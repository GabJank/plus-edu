<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <gradient
                android:angle="135"
                android:endColor="@color/verde_limao_claro_translucido"
                android:startColor="@color/amarelo_queimado_translucido"
                android:type="linear" />

            <corners android:radius="10dp" />

        </shape>
    </item>

</layer-list>
